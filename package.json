{
    "name": "treinamento.test.e2e",
    "version": "2.0.0",
    "description": "Project from WebdriverIOv5, Cucumber, TypeScript and Appium to system Treinamento",
    "homepage": "http://www.treinamento.net/",
    "main": "index.js",
    "keywords": [
        "webdriverio",
        "cucumber",
        "typescript",
        "testing",
        "behaviour driven development",
        "bdd",
        "mobile",
        "appium",
        "selenium-standalone",
        "automation testing"
    ],
    "scripts": {
        "test": "npm run clean-newman-allure-reports && wdio ./conf/local.conf.js && npm run allure-report",
        "test:debug": "npm run clean-newman-allure-reports && DEBUG=true wdio ./conf/local.conf.js",
        "allure-report": "allure generate ./allure-results --clean && allure open",
        "concurrently": "./node_modules/.bin/concurrently",
        "clean-newman-allure-reports": "node clean.js",
        "app-test": "wdio ./conf/app.conf.js",
        "test-app": "npm run app-test && npm run allure-report",
        "connect-db": "node ./support/util/db/db-localhost/connect-backoffice.js"
    },
    "author": "Fernanda Ferreira <fernandaferreiraqa@gmail.com>",
    "license": "Treinamento",
    "devDependencies": {
        "@types/chai": "4.2.4",
        "@types/cucumber": "^4.0.7",
        "@types/jsdom": "^12.2.4",
        "@types/reflect-metadata": "^0.1.0",
        "@types/webdriverio": "^5.0.0",
        "@wdio/allure-reporter": "5.15.3",
        "@wdio/appium-service": "^5.16.5",
        "@wdio/cli": "5.15.4",
        "@wdio/cucumber-framework": "5.15.1",
        "@wdio/local-runner": "5.15.4",
        "@wdio/selenium-standalone-service": "5.15.0",
        "@wdio/spec-reporter": "5.15.2",
        "@wdio/sync": "5.15.1",
        "allure-commandline": "^2.13.0",
        "appium": "^1.15.1",
        "async": "^3.1.0",
        "ava": "^2.4.0",
        "babel-jest": "^25.0.0",
        "babel-polyfill": "~6.20.0",
        "babel-preset-es2015": "~6.18.0",
        "babel-register": "~6.18.0",
        "bluebird": "^2.2.2",
        "chai": "^4.2.0",
        "coveralls": "^3.0.2",
        "cucumber-tsflow": "^3.2.0",
        "dotenv": "latest",
        "eslint": "~3.13.1",
        "eslint-config-airbnb-base": "~11.0.1",
        "eslint-plugin-import": "~2.9.0",
        "grunt": "latest",
        "grunt-cucumber": "latest",
        "http-server": "^0.12.1",
        "husky": "^0.13.3",
        "jest": "^25.0.0",
        "lint-staged": "^3.4.2",
        "mssql": "^6.0.1",
        "nyc": "^14.1.1",
        "prettier": "1.18.2",
        "rimraf": "2.6.3",
        "shelljs": "^0.8.3",
        "tedious": "6.6.5",
        "ts-node": "8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.10.0",
        "typescript": "3.6.4",
        "typescript-node": "latest",
        "webdriverio": "5.15.4",
        "webdriverio-client": "^3.0.0",
        "xo": "^0.18.2"
    },
    "xo": {
        "semicolon": false,
        "space": true,
        "esnext": true,
        "rules": {
            "no-mixed-operators": 0,
            "object-curly-spacing": 0
        }
    },
    "repository": "https://github.com/fcffc/treinamento.test.e2e.git",
    "dependencies": {
        "@types/newman": "^3.11.1",
        "concurrently": "^5.0.2",
        "jsdom": "15.2.0",
